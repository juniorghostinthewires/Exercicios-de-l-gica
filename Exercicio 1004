programa  
{  
    funcao inicio()       {  
        inteiro a,b,prod
        
              

             escreva("Digite aqui o primeiro valor, por gentileza:  ")  
              leia(a)   
              escreva("Digite aqui o segundo valor: ")
              leia(b)      
              prod= a * b
              escreva("Seu produto sera: ", prod)
              
        }  
        }  
